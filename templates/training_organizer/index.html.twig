{% extends 'base.html.twig' %}

{% block title %}TrainingOrganizer index{% endblock %}

{% block body %}
    <h1>TrainingOrganizer index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>CreatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for training_organizer in training_organizers %}
            <tr>
                <td>{{ training_organizer.id }}</td>
                <td>{{ training_organizer.createdAt ? training_organizer.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_training_organizer_show', {'id': training_organizer.id}) }}">show</a>
                    <a href="{{ path('app_training_organizer_edit', {'id': training_organizer.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_training_organizer_new') }}">Create new</a>
{% endblock %}

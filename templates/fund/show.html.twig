{% extends 'base.html.twig' %}

{% block title %}Fund{% endblock %}

{% block body %}
    <h1>Fund</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fund.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ fund.name }}</td>
            </tr>
            <tr>
                <th>Budget</th>
                <td>{{ fund.budget }}</td>
            </tr>
            <tr>
                <th>AmountInWord</th>
                <td>{{ fund.amountInWord }}</td>
            </tr>
            <tr>
                <th>FiscalYear</th>
                <td>{{ fund.fiscalYear ? fund.fiscalYear|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>CommencedYear</th>
                <td>{{ fund.commencedYear ? fund.commencedYear|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>FundEndDate</th>
                <td>{{ fund.fundEndDate ? fund.fundEndDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ fund.createdAt ? fund.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ fund.updatedAt ? fund.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_fund_index') }}">back to list</a>

    <a href="{{ path('app_fund_edit', {'id': fund.id}) }}">edit</a>

    {{ include('fund/_delete_form.html.twig') }}
{% endblock %}

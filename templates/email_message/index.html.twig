{% extends 'base.html.twig' %}

{% block title %}EmailMessage index{% endblock %}

{% block body %}
    <h1>EmailMessage index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email_key</th>
                <th>Subject</th>
                <th>Body</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for email_message in email_messages %}
            <tr>
                <td>{{ email_message.id }}</td>
                <td>{{ email_message.emailKey }}</td>
                <td>{{ email_message.subject }}</td>
                <td>{{ email_message.body }}</td>
                <td>
                    <a href="{{ path('app_email_message_show', {'id': email_message.id}) }}">show</a>
                    <a href="{{ path('app_email_message_edit', {'id': email_message.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_email_message_new') }}">Create new</a>
{% endblock %}

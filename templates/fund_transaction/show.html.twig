{% extends 'base.html.twig' %}

{% block title %}FundTransaction
{% endblock %}

{% block body %}
	<h1>FundTransaction</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ fund_transaction.id }}</td>
			</tr>
			<tr>
				<th>Deducted</th>
				<td>{{ fund_transaction.deducted }}</td>
			</tr>
			<tr>
				<th>CurrentBalance</th>
				<td>{{ fund_transaction.currentBalance }}</td>
			</tr>
			<tr>
				<th>DeductedAt</th>
				<td>{{ fund_transaction.deductedAt ? fund_transaction.deductedAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>Reason</th>
				<td>{{ fund_transaction.reason }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ fund_transaction.name }}</td>
			</tr>
			<tr>
				<th>ReferenceNubmer</th>
				<td>{{ fund_transaction.referenceNubmer }}</td>
				<td> 
				 
 <img  width="60" height="60" src="data:image/png;base64,
{{ barcode({code:  fund_transaction.referenceNubmer , type: 'datamatrix', format: 'png', width: 10, height: 10, color: [127, 127, 127]}) }}
" />
				</td>

			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_fund_transaction_index') }}">back to list</a>

	<a href="{{ path('app_fund_transaction_edit', {'id': fund_transaction.id}) }}">edit</a>

	{{ include('fund_transaction/_delete_form.html.twig') }}
{% endblock %}
